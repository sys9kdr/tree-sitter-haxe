{
  "grammars": [
    {
      "name": "haxe",
      "camelcase": "Haxe",
      "scope": "source.hx",
      "path": ".",
      "file-types": [
        "hx"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "injection-regex": "^(hx|haxe)$"
    }
  ],
  "metadata": {
    "version": "0.13.0",
    "license": "MIT",
    "description": "A tree sitter parser for haxe.",
    "authors": [
      {
        "name": "vantreeseba@gmail.com"
      }
    ],
    "links": {
      "repository": "git+https://github.com/vantreeseba/tree-sitter-haxe.git"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}